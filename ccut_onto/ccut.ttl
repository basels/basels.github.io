@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml:   <http://www.w3.org/XML/1998/namespace> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix ccut:  <https://basels.github.io/ccut_onto/ccut.ttl#> .
@prefix qudtp: <http://www.qudt.org/qudt/owl/1.0.0/qudt/Properties.html#> .
@prefix qudtu: <http://www.qudt.org/qudt/owl/1.0.0/unit/Instances.html#> .
@prefix qudt:  <http://www.qudt.org/qudt/owl/1.0.0/qudt/#> .


ccut:atomicUnit a rdfs:Class ;
    rdfs:label                      "Atomic Unit"@en ;
    rdfs:comment                    "A class to represent a single unit symbol which may be modified by additional elements such as exponents or prefixes"@en ;
    qudtp:symbol                    xsd:string ;
    qudtp:conversionMultiplier      xsd:double ;
    qudtp:conversionOffset          xsd:double ;
    qudtp:quantityKind              qudt:QuantityKind ;
    ccut:prefix                     qudt:QuantityKind ;
    ccut:prefixConversionMultiplier xsd:double ;
    ccut:prefixConversionOffset     xsd:double ;
    ccut:exponent                   xsd:double ;
    ccut:multiplier                 xsd:double ;
    ccut:hasDimension               xsd:string .

ccut:compoundUnit a rdfs:Class ;
    rdfs:label                      "Compound Unit"@en ;
    rdfs:comment                    "A class to represent a complex unit that is composed from two or more atomic units with some relationship between them"@en ;
    qudtp:abbreviation              xsd:string ;
    ccut:hasPart                    ccut:atomicUnit ;
    ccut:hasDimension               xsd:string .

ccut:hasDimension a rdf:Property ;
    rdfs:label      "Has Dimension"@en ;
    rdfs:comment    "Associates an atomic or compound unit with a dimension string"@en ;
    rdfs:range      xsd:string .

ccut:hasPart a rdf:Property ;
    rdfs:label      "Has Part"@en ;
    rdfs:comment    "Associates a compound unit with atomic units that compose it"@en ;
    rdfs:range      ccut:atomicUnit .

ccut:prefix a rdf:Property ;
    rdfs:label      "Prefix"@en ;
    rdfs:comment    "Associates an SI (International System of Units) Prefix URI to some atomic unit"@en ;
    rdfs:range      qudt:QuantityKind .

ccut:prefixConversionMultiplier a rdf:Property ;
    rdfs:label      "Prefix Conversion Multiplier"@en ;
    rdfs:comment    "Associates an SI (International System of Units) Prefix Multiplier to some atomic unit"@en ;
    rdfs:range      xsd:double .

ccut:prefixConversionOffset a rdf:Property ;
    rdfs:label      "Prefix Conversion Offset"@en ;
    rdfs:comment    "Associates an SI (International System of Units) Prefix Offset to some atomic unit"@en ;
    rdfs:range      xsd:double .

ccut:exponent a rdf:Property ;
    rdfs:label      "Exponent"@en ;
    rdfs:comment    "Associates an exponent (dimension exponent) to some atomic unit"@en ;
    rdfs:range      xsd:double .

ccut:multiplier a rdf:Property ;
    rdfs:label      "Multiplier"@en ;
    rdfs:comment    "Associates a multiplier (dimension prefix multiplier) to some atomic unit"@en ;
    rdfs:range      xsd:double .